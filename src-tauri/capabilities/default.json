{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Permissions for the FS25 Save Editor",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "opener:default",
    "opener:allow-open-path",
    "store:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPDATA/cache/**" }]
    },
    {
      "identifier": "fs:allow-read-file",
      "allow": [{ "path": "$APPDATA/cache/**" }]
    }
  ]
}
